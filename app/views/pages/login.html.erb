<!-- app/views/pages/login.html.erb -->
<%= form_for :users, :url => { :action => "login" } do |f| %>
  <%= f.label "Username: " %>
  <%= f.text_field :username %><br />
  <%= f.label "Password: " %>
  <%= f.password_field :password %><br />
  <%= f.submit "Submit" %><br>
  <%= link_to "Public Page", :controller => "suggestions_users",
    :action => "index" %>
<% end %>
