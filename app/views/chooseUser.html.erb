<!-- app/views/users/chooseUser.html.erb -->
<h2>Choose user to edit</h2>
<% if cookies.signed[:user_id] == 1 %>

  <%= form_for :id, :url => {:action => "edit"} do |f|%>
    <%= f.collection_select @user, @users, :id, :name,
      options = {:prompt => "Choose a user"}, :class => "user" %><br />
    <%= f.submit "Submit" %>
  <% end %>
<% end %>
